

.input-item {
	width: 100%;
	height: 100rpx;
	border-bottom: 1rpx solid;
	padding: 0 20rpx;
	box-sizing: border-box;
}

.btn-item {
	margin: 40rpx;
	background-color: #15559a;
	color: #fff;
	letter-spacing: 8rpx;
}

.handle {
	display: flex;
	justify-content: space-between;
	gap: 20rpx;
}

.handle-item {
	background-color: #eee;
	border-radius: 15rpx;
	margin: 20rpx 0;
	color: #333;
}
